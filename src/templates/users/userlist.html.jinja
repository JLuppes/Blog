{% extends 'base.html.jinja'%}

{% block content %}

<h1>User List</h1>
{% if users %}
<ul>
    {% for user in users%}
    <li>
        <h3>User #<a href="{{url_for('mainNav.users.user_profile',id=user.id)}}">{{user.id}}</a></h3>
        <p>Name: {{user.display_name}}</p>
    </li>
    {% endfor %}
</ul>
{%else%}
<p>No users yet!</p>
{% endif %}

{% endblock %}